    "deploy:testnet": "hardhat --network testnet deploy --reset",
    "test": "hardhat test --no-compile",
    "mainnet": "hardhat console --network mainnet",
    "testnet": "hardhat console --network testnet",
    "test:case": "mocha --timeout 50000 --require hardhat/register --file tests/**/* --extension ts -g",
    "prepublishOnly": "yarn compile",
    "test:unit:latteNFT": "hardhat test --no-compile ./tests/unit/LatteNFT.test.ts",
    "test:unit:boosterConfig": "hardhat test --no-compile ./tests/unit/BoosterConfig.test.ts",
    "test:unit:latteSwapRouter": "hardhat test --no-compile ./tests/unit/LatteSwapRouter.test.ts",
